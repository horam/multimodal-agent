{
  "name": "multimodal-agent",
  "displayName": "Multimodal Agent",
  "description": "Generate Flutter code using multimodal-agent",
  "version": "0.9.0",
  "publisher": "horam",
  "engines": {
    "vscode": "^1.85.0"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "prepublish": "npm run compile",
    "test": "npm run compile && c8 node ./dist/test/runTest.js",
    "coverage": "npm run test"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "multimodalAgent.generateWidget",
        "title": "Multimodal Agent: Generate Widget"
      },
      {
        "command": "multimodalAgent.generateScreen",
        "title": "Multimodal Agent: Generate Screen"
      },
      {
        "command": "multimodalAgent.generateModel",
        "title": "Multimodal Agent: Generate Model"
      },
      {
        "command": "multimodalAgent.generateEnum",
        "title": "Multimodal Agent: Generate Enum"
      },
      {
        "command": "multimodalAgent.generateRepository",
        "title": "Multimodal Agent: Generate Repository"
      },
      {
        "command": "multimodalAgent.chat",
        "title": "Multimodal Agent: Chat"
      }
    ]
  },
  "configuration": {
    "title": "Multimodal Agent",
    "properties": {
      "multimodalAgent.serverUrl": {
        "type": "string",
        "default": "http://127.0.0.1:8000",
        "description": "Local multimodal-agent server URL"
      }
    }
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.1",
    "@types/vscode": "^1.107.0",
    "@vscode/test-electron": "^2.5.2",
    "axios": "^1.13.2",
    "c8": "^10.1.3",
    "glob": "^13.0.0",
    "mocha": "^11.7.5",
    "typescript": "^5.9.3"
  }
}
